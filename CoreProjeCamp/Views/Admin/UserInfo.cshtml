@using Entity.Identity
@model IEnumerable<AppUser>
@{
    ViewData["Title"] = "UserInfo";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<center style="margin-top:5px"><h3>"Kullanıcı Bilgilieri"</h3></center>
<br />
<table style="margin:auto" class="table table-bordered table-hover">
    <thead>
        <tr>
            <td>Id</td>
            <td>Adı</td>
            <td>Soyadı</td>
            <td>Kullanıcı Adı</td>
            <td>Email</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.SurName</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>
            </tr>
        }
        
    </tbody>
</table>
